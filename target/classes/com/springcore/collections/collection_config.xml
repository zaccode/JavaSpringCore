<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">



<!-- using beans context another way -->
<bean class="com.springcore.collections.Emp" name="student1">
<property name="name" value="Radhe"/>
	<property name="phone">
		<list>
			<value>745666</value>
			<value>987654</value>
			<value>23456789</value>
			<null/>
		</list>
	</property>
	
	<property name="address">
		<set>
			<value>Delhi</value>
			<value>Kanpur</value>
			<value>Lucknow</value>
		</set>
	</property>
	
	<property name="courses">
		<map>
			<entry key="Java" value="2Months" />
			<entry key="python" value="1.2Months" />
			<entry key="JavaScript" value="2.1Months" />
		</map>
	</property>
</bean>

</beans>





 
